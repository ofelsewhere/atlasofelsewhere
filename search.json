---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | strip_html }}",
      "categories" : "{{ post.categories | join: ' | ' }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    } 
    
    
  {% else post in site.posts %}
  
  {% unless forloop.last %},{% endunless %}

  {% endfor %}
]